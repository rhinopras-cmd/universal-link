{
  "mimeTypes": {
    ".json": "application/json"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/.well-known/*",
      "/apple-app-site-association"
    ]
  },
  "responseOverrides": {
    "apple-app-site-association": {
      "contentType": "application/json",
      "statusCode": 200
    }
  }
}